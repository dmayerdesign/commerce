{
  "name": "commerce",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "predev:ui": "npm run prebuild:ui:development",
    "predev:server": "npm run prebuild:server:development",
    "dev:ui": "ng serve",
    "dev:server": "nodemon --config nodemon.json",
    "start": "node dist/main.js",
    "prebuild:common": "export ENVIRONMENT=development; npm run generate:app_config; npm run generate:public_api",
    "prebuild:ui:development": "export ENVIRONMENT=development; npm run generate:app_config",
    "prebuild:server:development": "export ENVIRONMENT=development; npm run generate:app_config",
    "prebuild:ui:production": "export ENVIRONMENT=production; npm run generate:app_config; npm run test:ui",
    "prebuild:server:production": "export ENVIRONMENT=production; npm run generate:app_config; npm run test:server",
    "build:common": "ng build common",
    "build:ui:development": "ng build web --configuration=development && ng run web:ssr",
    "build:ui:production": "ng build web --configuration=production && ng run web:ssr:production",
    "build:server:development": "tsc -p server/tsconfig.server.json",
    "build:server:production": "tsc -p server/tsconfig.server.json",
    "build:development": "npm run build:ui:development && npm run build:server:development",
    "build:production": "npm run build:ui:production && npm run build:server:production",
    "test:ui:unit": "jest --config ./jest.ui-unit.config.js",
    "test:ui:e2e": "ng e2e",
    "test:ui": "npm run test:ui:unit && npm run test:ui:e2e",
    "test:server:unit:watch": "jest --config ./server/test/jest-unit.config.js --watch",
    "test:server:unit:cov": "jest --config ./server/test/jest-unit.config.js --coverage",
    "test:server:unit": "jest --config ./server/test/jest-unit.config.js",
    "test:server": "npm run test:server:unit",
    "test": "npm run test:ui && npm run test:server",
    "generate:app_config": "ts-node common/src/code-gen/generate-app-config.ts",
    "generate:public_api": "ts-node common/src/code-gen/generate-public_api.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "ng": "ng"
  },
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/platform-server": "7.0.2",
    "@angular/router": "~7.0.0",
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/ng-universal": "0.1.1",
    "@ng-toolkit/universal": "1.1.42",
    "@nguniversal/module-map-ngfactory-loader": "7.0.2",
    "core-js": "^2.5.4",
    "mongoose": "5.3.9",
    "mongoose-findorcreate": "3.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~6.3.3",
    "typescript": "~3.1.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "23.3.9",
    "@types/lodash": "4.14.117",
    "@types/mongoose": "5.3.0",
    "@types/node": "^10.7.1",
    "@types/recursive-readdir": "2.2.0",
    "@types/supertest": "^2.0.5",
    "codelyzer": "~4.5.0",
    "jasmine": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "23.6.0",
    "jest-preset-angular": "6.0.1",
    "lodash": "4.17.11",
    "ng-packagr": "^4.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "protractor": "~5.4.0",
    "recursive-readdir": "2.2.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "5.11.0",
    "typescript": "~3.1.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
