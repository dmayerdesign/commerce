{
  "name": "commerce",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev:ui": "ng serve",
    "dev:server": "ts-node -r tsconfig-paths/register --project server/tsconfig.server.json src/main.ts",
    "debug:server": "nodemon --config nodemon-debug.json",
    "prestart": "rimraf dist && build:server:production && build:ui",
    "start": "node dist/main.js",
    "build:ui": "ng build",
    "build:server:development": "webpack --config webpack.config.js",
    "build:server:production": "tsc -p server/src/tsconfig.server.json",
    "build:server": "tsc -p server/src/tsconfig.server.json",
    "test:server": "jest",
    "test:ui": "ng test",
    "test:ui:e2e": "ng e2e",
    "test:server:watch": "jest --watch",
    "test:server:cov": "jest --coverage",
    "test:server:e2e": "jest --config ./test/jest-e2e.json",
    "test": "npm run test:ui && npm run test:server",
    "lint": "ng lint; tslint -p tsconfig.json -c tslint.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "ng": "ng"
  },
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "core-js": "^2.5.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~6.3.3",
    "typescript": "~3.1.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@nestjs/testing": "^5.1.0",
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.5.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "5.11.0",
    "typescript": "~3.1.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
